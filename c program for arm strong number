#include<stdio.h>  
int main()    
{    
int n,r,s=0,t;    
printf("Enter the Number: ");    
scanf("%d",&n);    
t=n;    
while(n>0)    
{    
r=n%10;    
s=s+(r*r*r);    
n=n/10;    
}    
if(t==s)    
printf("\nArmstrong Number ");    
else    
printf("\nNot Armstrong Number");    
return 0;  
}   
